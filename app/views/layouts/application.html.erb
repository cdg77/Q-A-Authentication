<!DOCTYPE html>
<html>
<head>
  <title>QAAuthentication</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <div class="container">
    <div class="header">
      <h3><%= link_to "Sign Up", new_user_path %></h3>
    </div>
    <%= yield %>
    <br>
    <h2><%= flash[:notice] %></h2>
  </div>
</body>
</html>
